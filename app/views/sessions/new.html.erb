<h1>Rocket Airlines </h1>
<h2>Log In</h2>

  <%= form_for @user do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :password, "password:" %>
    <%= f.password_field :password %>

    <%= f.submit 'Sign In'%>

  <% end %>

<br>

<h4><%=link_to 'Create an Account', new_user_path %></h4>
