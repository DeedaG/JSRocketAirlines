<h1>Rocket Airlines </h1>
<h2>Log In</h2>

  <%= form_for @user, url: '/signin' do |f| %>

    <%= f.label :name, "name:" %>
    <%= f.text_field :name %>

    <%= f.label :email, "email:" %>
    <%= f.password_field :email %>

    <%= f.label :password, "password:" %>
    <%= f.password_field :password %>

    <%= f.submit 'Log In'%>

  <% end %>

<br>

<h4><%=link_to 'Create an Account', new_user_path %></h4>
